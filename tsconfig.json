{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": ["resources/ts/*"]
        },
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["resources/ts"]
}
